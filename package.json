{
  "name": "gameboy_emulator",
  "version": "0.1.0",
  "description": "Play around with the MessagePack library for Typescript",
  "author": "Michael West",
  "license": "MIT",
  "main": "dist/emulator.js",
  "scripts": {
    "build": "webpack -p",
    "startw": "webpack && qode ./dist/main.js",
    "gb": "./node_modules/.bin/qode dist/main.js",
    "start": "tsc --p . --watch",
    "emu:debug": "./node_modules/.bin/ts-node src/app/gameboy.ts roms/tetris.gb debug",
    "emu": "./node_modules/.bin/ts-node src/app/gameboy.ts roms/tetris.gb",
    "emudebug": "emu() { ./node_modules/.bin/ts-node src/app/gameboy.ts \"$1\" debug; }; emu",
    "run:server": "http-server src/js --cors -p 8082"
  },
  "dependencies": {
    "@nodegui/nodegui": "^0.19.0",
    "@types/jquery": "^3.3.33",
    "@types/text-encoding": "0.0.35",
    "@types/ws": "^7.2.3",
    "clean-webpack-plugin": "^3.0.0",
    "lodash": "^4.17.15",
    "native-addon-loader": "^2.0.1",
    "readline": "^1.3.0",
    "readline-sync": "^1.4.10",
    "text-encoder": "0.0.4",
    "text-encoding": "^0.7.0",
    "ts-node": "^8.8.2",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "ws": "^7.2.3"
  },
  "devDependencies": {
    "@types/node": "^13.7.1",
    "typescript": "^3.3.3333"
  }
}
